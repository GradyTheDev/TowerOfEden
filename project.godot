; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Tower of Eden"
config/description="Game for the SMVM 2023 by terraquad and 9 other amazing developers."
run/main_scene="res://other/launcher/launcher.tscn"
config/features=PackedStringArray("4.1", "Mobile")
config/icon="res://icon.svg"
config/version="v0.0.0.A"
config/build_date="Running inside Editor"

[audio]

buses/default_bus_layout="res://other/default_bus_layout.tres"

[autoload]

SceneManager="*res://autoloads/scene_manager.gd"
Globals="*res://autoloads/globals.gd"
SettingsManager="*res://autoloads/settings_manager.gd"
SavesManager="*res://autoloads/saves_manager.gd"
Controls="*res://autoloads/controls.gd"
SoundManager="*res://autoloads/sound_manager/sound_manager.tscn"

[debug]

file_logging/max_log_files=1
gdscript/warnings/enable=false

[display]

window/size/viewport_width=960
window/size/viewport_height=540
window/stretch/mode="viewport"
window/stretch/aspect="keep_width"
window/vsync/vsync_mode=2

[editor]

movie_writer/mjpeg_quality=0.9
movie_writer/movie_file="../movie.avi"
movie_writer/disable_vsync=true
naming/node_name_num_separator=2

[editor_plugins]

enabled=PackedStringArray("res://addons/buildscripts/plugin.cfg", "res://addons/imjp94.yafsm/plugin.cfg", "res://addons/ldtk/plugin.cfg")

